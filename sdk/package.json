{
  "name": "monoidentity",
  "version": "0.11.2",
  "repository": "KTibow/monoidentity",
  "author": {
    "name": "KTibow"
  },
  "homepage": "https://github.com/KTibow/monoidentity",
  "funding": "https://github.com/sponsors/KTibow",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && rm -rf predist && node --env-file-if-exists=.env node_modules/rolldown/bin/cli.mjs -c && svelte-package -i predist -t && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "knip": "knip"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./Monoidentity.svelte": {
      "types": "./dist/Monoidentity.svelte.d.ts",
      "svelte": "./dist/Monoidentity.svelte"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "aws4fetch": "^1.0.20",
    "devalue": "^5.3.2",
    "idb-keyval": "^6.2.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.43.7",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/wicg-file-system-access": "^2023.10.6",
    "fast-xml-parser": "^5.2.5",
    "knip": "^5.64.1",
    "monoserve": "^2.2.0",
    "publint": "^0.3.13",
    "rolldown": "1.0.0-beta.40",
    "school-districts": "^2.0.0",
    "svelte": "^5.39.8",
    "svelte-check": "^4.3.2",
    "tinyglobby": "^0.2.15",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "keywords": [
    "svelte"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
