{
  "name": "monoidentity",
  "version": "0.19.2",
  "license": "ISC",
  "repository": "KTibow/monoidentity",
  "author": {
    "name": "KTibow"
  },
  "homepage": "https://github.com/KTibow/monoidentity",
  "funding": "https://github.com/sponsors/KTibow",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm run prepack && pnpm --prefix firebase-functions run build",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && rm -rf predist && node --env-file-if-exists=.env node_modules/rolldown/bin/cli.mjs -c && svelte-package -i predist -t && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "knip": "knip"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/+client.d.ts",
      "svelte": "./dist/+client.js"
    },
    "./server": {
      "types": "./dist/+server.d.ts",
      "import": "./dist/+server.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "aws4fetch": "^1.0.20",
    "devalue": "^5.5.0",
    "fast-studentvue": "^2.1.1",
    "idb-keyval": "^6.2.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/wicg-file-system-access": "^2023.10.7",
    "knip": "^5.69.1",
    "monoserve": "^3.2.2",
    "publint": "^0.3.15",
    "rolldown": "^1.0.0-beta.47",
    "school-districts": "^5.0.1",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.4",
    "tinyglobby": "^0.2.15",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "keywords": [
    "svelte"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
